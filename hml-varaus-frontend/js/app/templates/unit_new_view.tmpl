<div class="row">
    <div class="col-md-6">
        <form id="new-unit-form">
            <div class="form-group">
                <label for="unit-name">Nimi</label>
                <input name="name" type="text" class="form-control" id="unit-name">
            </div>
            <div class="form-group">
                <label for="unit-address">Katuosoite</label>
                <input name="street_address" type="text" class="form-control" id="unit-address">
            </div>
            <div class="form-group">
                <label for="unit-zip">Postinumero</label>
                <input name="address_zip" type="number" class="form-control" id="unit-zip">
            </div>
            <div class="form-group">
                <label for="unit-email">Sähköpostiosoite</label>
                <input name="email" type="email" class="form-control" id="unit-email">
            </div>
            <div class="form-group">
                <label for="unit-phone">Puhelinnumero</label>
                <input name="phone" type="number" class="form-control" id="unit-phone">
            </div>
          <button id="unit-submit" type="submit" class="btn btn-default submit-button">Lisää kohde</button>
        </form>
    </div>
    <div class="col-md-6">
    </div>
</div>

